{
    "name": "macsidigital/laravel-zoom",
    "description": "Laravel Zoom package",
    "homepage": "https://github.com/MacsiDigital/laravel-zoom",
    "keywords": ["macsidigital", "laravel-zoom", "zoom", "laravel", "api", "client"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Colin Hall",
            "email": "colin@macsi.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3", 
        "guzzlehttp/guzzle": "~6.0|~5.0|~4.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "macsidigital/laravel-api-client": "v1.x-dev as 1.0.10"
    },
    "require-dev": {
        "orchestra/testbench": "^3.0|^4.0|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MacsiDigital\\Zoom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MacsiDigital\\Zoom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MacsiDigital\\Zoom\\Providers\\ZoomServiceProvider"
            ],
            "aliases": {
                "Zoom": "MacsiDigital\\Zoom\\Facades\\Zoom"
            }
        }
    },
    "minimum-stability" : "dev",
    "repositories": [
        {
            "type": "path",
            "url":  "../laravel-api-client",
            "options": {
              "symlink": true
            }
        },
        {
            "type": "path",
            "url":  "../laravel-oauth2-client",
            "options": {
              "symlink": true
            }
        }
    ]
}
